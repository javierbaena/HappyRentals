<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width" />
        <title>@Page.Title</title>
        <link href="~/Content/Site.css" type="text/css" rel="stylesheet" />
        @RenderSection("head", required: false)
    </head>
    <body>
        <div id="header">
            <div class="title">HAPPY RENTALS</div>
        </div>
        <div id="categories">
            @Html.ActionLink("Home Owners", "List", "HomeOwner")
            @Html.ActionLink("Homes", "List", "Home")
        </div>
        <div id="content">
            @RenderBody()
        </div>
    </body>
</html>
